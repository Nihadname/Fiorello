@model IEnumerable<Blog>
@foreach(var item in Model)
{
    <li class="d-flex px-3">
        <a asp-controller="Blog" asp-action="detail" asp-route-id="@item.Id" class="text-decoration-none">
            <div class="d-flex gap-1 align-items-center gap-3">
                @item.Title
                <img src="~/img/@item.ImageUrl" class="img-fluid" width="60" height="60"  alt="">
            </div>

        </a>

    </li>
}